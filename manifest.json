{
    "name": "WhatsApp Spammer",
    "version": "1.0",
    "manifest_version": 2,
    "author": "Junaid Shaikh",
    "description": "Chrome extension for WhatsApp",
    "icons":{
      "128":"/assets/icons/icon.png"
      },
    "background": {
      "scripts": ["assets/js/background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_title": "WhatsApp Spammer",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "storage"
    ]
  }